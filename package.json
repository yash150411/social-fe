    "tslint-react": "^4.1.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off",
          "react/react-in-jsx-scope": "off"
        },
        "extends": [
          "eslint:recommended",
          "plugin:react/recommended",
          "plugin:prettier/recommended"
        ]
      }
    ]
  }
}
